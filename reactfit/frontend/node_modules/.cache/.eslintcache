ength === 0) {\n      AuthService.login(this.state.username, this.state.password).then(\n        () => {\n          console.log(this.props);\n          this.props.history.push(\"/profile\");\n          window.location.reload();\n        },\n        (error) => {\n          const resMessage =\n            (error.response &&\n              error.response.data &&\n              error.response.data.message) ||\n            error.message ||\n            error.toString();\n          this.setState({\n            loading: false,\n            message: resMessage,\n          });\n        }\n      );\n    } else {\n      this.setState({\n        loading: false,\n      });\n    }\n  }\n  render() {\n    return (\n      <div className=\"col-md-12\">\n        <div className=\"card card-container\">\n          <img\n            src=\"//ssl.gstatic.com/accounts/ui/avatar_2x.png\"\n            alt=\"profile-img\"\n            className=\"profile-img-card\"\n          />\n          <Form\n            onSubmit={this.handleLogin}\n            ref={(c) => {\n              this.form = c;\n            }}\n          >\n            <div className=\"form-group\">\n              <label htmlFor=\"username\">Username</label>\n              <Input\n                type=\"text\"\n                className=\"form-control\"\n                name=\"username\"\n                value={this.state.username}\n                onChange={this.onChangeUsername}\n                validations={[required]}\n              />\n            </div>\n            <div className=\"form-group\">\n              <label htmlFor=\"password\">Password</label>\n              <Input\n                type=\"password\"\n                className=\"form-control\"\n                name=\"password\"\n                value={this.state.password}\n                onChange={this.onChangePassword}\n                validations={[required]}\n              />\n            </div>\n            <div className=\"form-group\">\n              <button\n                className=\"btn btn-primary btn-block\"\n                disabled={this.state.loading}\n              >\n                {this.state.loading && (\n                  <span className=\"spinner-border spinner-border-sm\"></span>\n                )}\n                <span>Login</span>\n              </button>\n            </div>\n            {this.state.message && (\n              <div className=\"form-group\">\n                <div className=\"alert alert-danger\" role=\"alert\">\n                  {this.state.message}\n                </div>\n              </div>\n            )}\n            <CheckButton\n              style={{ display: \"none\" }}\n              ref={(c) => {\n                this.checkBtn = c;\n              }}\n            />\n          </Form>\n        </div>\n      </div>\n    );\n  }\n}\n","/home/danailamarian/Desktop/Proiectul de licenta/reactfit/frontend/src/components/home.component.js",[],[],"/home/danailamarian/Desktop/Proiectul de licenta/reactfit/frontend/src/components/board-user.component.js",[],[],"/home/danailamarian/Desktop/Proiectul de licenta/reactfit/frontend/src/components/board-moderator.component.js",[],[],"/home/danailamarian/Desktop/Proiectul de licenta/reactfit/frontend/src/components/register.component.js",[],[],"/home/danailamarian/Desktop/Proiectul de licenta/reactfit/frontend/src/components/profile.component.js",[],[],"/home/danailamarian/Desktop/Proiectul de licenta/reactfit/frontend/src/components/board-admin.component.js",[],[],"/home/danailamarian/Desktop/Proiectul de licenta/reactfit/frontend/src/services/auth.service.js",[],[],"/home/danailamarian/Desktop/Proiectul de licenta/reactfit/frontend/src/services/user.service.js",[],[],"/home/danailamarian/Desktop/Proiectul de licenta/reactfit/frontend/src/services/auth-header.js",[],[],{"ruleId":"69","severity":1,"message":"70","line":6,"column":10,"nodeType":"71","messageId":"72","endLine":6,"endColumn":21},"no-unused-vars","'useNavigate' is defined